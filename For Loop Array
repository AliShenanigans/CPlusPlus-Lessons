/* multi dimensional for loop array */
#include <iostream>
using namespace std;

int main()
{
    int bucky[3][3];
    int i=0; int j=0; int n=0;
    //row i column j
    cout << "Element  -  Value" << endl;
    for (int i= 0; i<=2; i++)
        {
        for (int j= 0; j<=2; j++)
        {
        bucky[i][j] = n;
        cout << "i,j = " << i << "," << j << "   value=" << bucky[i][j] << endl;
        n++;
        }
    }
}
